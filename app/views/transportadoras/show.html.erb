<h2>Transportadoras</h2>

<div class="page-header">
  <%= link_to transportadoras_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_transportadora_path(@transportadora), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h2>Mostrar</h2>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @transportadora.nome %></dd>
  <dt>CNPJ:</dt>
  <dd><%= @transportadora.cnpj %></dd>
  <dt>IE:</dt>
  <dd><%= @transportadora.ie %></dd>
  <dt>Endere√ßo:</dt>
  <dd><%= @transportadora.endereco %></dd>
  <dt>Bairro:</dt>
  <dd><%= @transportadora.bairro %></dd>
  <dt>Cidade:</dt>
  <dd><%= @transportadora.cidade %></dd>
  <dt>CEP:</dt>
  <dd><%= @transportadora.cep %></dd>
  <dt>UF:</dt>
  <dd><%= @transportadora.uf %></dd>
</dl>
