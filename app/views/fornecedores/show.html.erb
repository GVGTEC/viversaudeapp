<h2>Fornecedores</h2>

<div class="page-header">
  <%= link_to fornecedores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_fornecedor_path(@fornecedor), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h2>Mostrar</h2>
</div>

<dl class="dl-horizontal">
  <dt>Nome/Razão Social:</dt>
  <dd><%= @fornecedor.nome %></dd>
  <dt>Pessoa:</dt>
  <dd><%= @fornecedor.pessoa %></dd>
  <dt>CPF:</dt>
  <dd><%= @fornecedor.cpf %></dd>
  <dt>RG:</dt>
  <dd><%= @fornecedor.rg %></dd>
  <dt>CNPJ:</dt>
  <dd><%= @fornecedor.cnpj %></dd>
  <dt>IE:</dt>
  <dd><%= @fornecedor.ie %></dd>
  <dt>Endereço:</dt>
  <dd><%= @fornecedor.endereco %></dd>
  <dt>Bairro:</dt>
  <dd><%= @fornecedor.bairro %></dd>
  <dt>Cidade:</dt>
  <dd><%= @fornecedor.cidade %></dd>
  <dt>CEP:</dt>
  <dd><%= @fornecedor.cep %></dd>
  <dt>UF:</dt>
  <dd><%= @fornecedor.uf %></dd>
  <dt>Email:</dt>
  <dd><%= @fornecedor.email %></dd>
</dl>
